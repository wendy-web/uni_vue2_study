/* 活动说明 */
.act-explain-box {
  margin: 60rpx 25rpx 0 25rpx;
  position: relative;
  padding: 10rpx;
  background: #ffe2bc;
  border-radius: 20rpx;
  z-index: 1;
}

.act-explain-box .icon-act {
  position: absolute;
  width: 376rpx;
  height: 101rpx;
  top: -30rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
}

.act-explain-box .act-rule-tag-box {
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 1;
  font-size: 40rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Bold;
  font-weight: 700;
  text-align: center;
  color: #d58418;
  padding-top: 10rpx;
  box-sizing: border-box;
  width: 378rpx;
  height: 90rpx;
  position: absolute;
  left: 0;
  right: 0;
  top: -20rpx;
  margin: 0 auto;
}

.act-rule-tag-box .act_rule_tag {
  width: 378rpx;
  height: 90rpx;
  position: absolute;
  z-index: -1;
  top: 0;
  left: 0;
  right: 0;
  margin: 0 auto;
}

.act-explain-box .inner-box {
  padding: 64rpx 34rpx 34rpx 34rpx;
  background: #fefbf2;
  border: 0.8rpx solid #fea910;
  border-radius: 20rpx;
}

.inner-box .join-box {
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin-top: 10rpx;
}

.join-box .join-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  font-family: Source Han Sans SC, Source Han Sans SC-Medium;
  font-weight: 500;
  text-align: center;
  color: #000018;
}

.join-item .icon_people {
  width: 78rpx;
  height: 78rpx;
}

.join-item .icon_unbox {
  width: 78rpx;
  height: 78rpx;
}

.inner-box .title {
  font-size: 27rpx;
  color: #ff4200;
  margin-top: 34rpx;
}

.color-black {
  color: #000000;
  font-size: 27rpx;
  margin-top: 34rpx;
}

.inner-box .content {
  font-size: 27rpx;
  color: #000000;
  letter-spacing: 0px;
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}

.inner-box .content .content-left {
  display: flex;
  align-items: flex-start;
  justify-content: center;
}

.inner-box .content .bg-num {
  width: 29rpx;
  height: 27rpx;
  line-height: 27rpx;
  text-align: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  font-size: 20rpx;
  color: #ffffff;
  margin-top: 8rpx;
}

.content .content-right {
  flex: 0.96;
  text-align: justify;
}

.prize-box {
  display: flex;
  flex-direction: column;
  font-size: 24rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  color: #000018;
  width: 100%;
}

.prize-box .prize-head {
  font-size: 24rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  color: #d58418;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
}

.prize-head>view,
.prize-item>view {
  flex: 0.3;
  flex-shrink: 0;
  text-align: center;
}

.prize-head>.prize-level,
.prize-item>.prize-level {
  flex: 0.4;
  flex-shrink: 0;
  text-align: center;
}

.prize-box .prize-item {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 50rpx;
  text-align: center;
}

/* 返回顶部 */
