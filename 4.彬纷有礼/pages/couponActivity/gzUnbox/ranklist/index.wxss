.no-data {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-weight: 500;

  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  color: #000018;
}

.list-box {
  box-sizing: border-box;
  flex: 1;
  flex-shrink: 0;
  border-radius: 0 0 35rpx 35rpx;
  z-index: 10;
  overflow: scroll;
  min-height: 500rpx;

}

/* 2023年5月28日 6月开箱冲榜活动 */
.table-head-title {
  font-size: 28rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Bold;
  font-weight: 700;
  text-align: left;
  color: #4e4d52;
  line-height: 34rpx;
}

.text-stroke {
  -webkit-text-fill-color: white;
  -webkit-text-stroke-width: 1rpx;
  -webkit-text-stroke-color: #A25037;
}

.myrank-num {
  text-align: center;
  font-size: 36rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Heavy;
  font-weight: 800;
  color: #CB3A00;
}

/* 2023年8月26日 广州开箱活动 */
.rank-head-box {
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  font-size: 22rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #ffffff;
  padding: 0 30rpx;
  box-sizing: border-box;
  z-index: 1;
  min-height: 220rpx;
  overflow: hidden;
  flex-shrink: 0;
}

.rank-head-box .rank_title {
  width: 360rpx;
  height: 144rpx;
  display: block;
}

.bg_rank_grail {
  width: 272rpx;
  height: 272rpx;
  position: absolute;
  right: 20rpx;
  bottom: -80rpx;
  z-index: -1;
}
.rank_icon{
  width: 60rpx;
  height: 48rpx;
}