/* 开红包弹窗 animate */
.getRewardPop .fromTop {
  display: block;
  animation: fromtop ease 0.5s forwards
}

.getRewardPop .fromtop {
  display: block;
  animation: fromtop2 ease 0.5s forwards
}

.getRewardPop .backTop {
  animation: backtop ease 0.4s forwards;
}

@keyframes fromtop {
  from {
    transform: translate3d(0, -170%, 0);
  }

  to {
    transform: translate3d(0, 0, 0);
  }
}

@keyframes fromtop2 {
  from {
    transform: translate3d(0, -300%, 0);
  }

  to {
    transform: translate3d(0, 0, 0);
  }
}

@keyframes backtop {
  from {
    transform: translate3d(0, 0, 0);
  }

  to {
    transform: translate3d(0, -170%, 0);
  }
}

