/* 2023年8月14日 产品兑换券统计 */
.flex-col-center {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.box-coupon-item {
  position: relative;
  z-index: 1;
  height: 208rpx;
  display: flex;
  flex-direction: column;
  /* justify-content: center; */
  box-sizing: border-box;
}

.box-coupon-item .bg-box-coupon {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
}

.box-coupon-item .box-coupon-inner {
  position: relative;
  margin: 16rpx 20rpx 0rpx 40rpx;
  box-sizing: border-box;
  display: flex;
  height: 160rpx;
  /* background-color: teal; */
}

.box-coupon-item .box-coupon-bar {
  width: 33rpx;
  height: 166rpx;
}

.box-coupon-item-left {
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  text-align: left;
  color: #000000;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 470rpx;
}

.box-coupon-item-left .explain {
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Medium;
  font-weight: 500;
  text-align: left;
  color: #999999;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.box-coupon-item-right{
  position: relative;
  display: flex;
  align-items: center;
}
.box-coupon-item-right .box-coupon-stamp{
  width: 165rpx;
  height: 127rpx;
}

.bg-type9-stamp {
  position: absolute;
  bottom: 0rpx;
  top: 0rpx;
  margin: auto;
  width: 218rpx;
  height: 214rpx;
}
.actlist-item-box{
  display: flex;
  align-items: center;
  width: 100%;
}