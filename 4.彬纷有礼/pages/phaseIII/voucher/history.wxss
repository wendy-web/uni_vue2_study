/* pages/phaseIII/voucher/history.wxss */
page {
  background-color: #f5f5f5;
  height: 100%;
}

.page {
  background-color: #f5f5f5;
}

.page-white {

  background-color: #ffffff;
}

.content {
  overflow-y: scroll;
  z-index: 0;
  min-height: 460rpx;
  box-sizing: border-box;
  position: relative;
}

.tab-stick-box {
  position: sticky;
  top: 0;
  z-index: 100;
}

.tabs {
  color: #000000;
  display: flex;
  align-items: center;
  justify-content: space-around;
  padding-left: 20rpx;
  padding-right: 20rpx;
  background-color: #ffffff;
  /* height: 108rpx; */
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;

}

.tabs .tab-item {
  height: 90rpx;
  min-width: 200rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}

.tab-item .tab-item-unit {
  color: #666666;
}

.tab-item .icon-arrow-up {
  width: 40rpx;
  height: 16rpx;
}

.active {
  color: #e01b14;

}

.unactive {
  display: flex;
  justify-content: flex-start;
  flex-direction: column;
  align-items: center;
}

.tab-item-num {
  color: #000018;
  font-size: 24rpx;
}

.bottom_line {
  display: inline-block;
  width: 41px;
  height: 1px;
  opacity: 1;
  border-bottom: 6rpx solid #e3001b;
  border-radius: 50rpx;
}

.unbottom_line {
  display: inline-block;
  width: 41rpx;
  height: 1px;
  opacity: 1;
  border-bottom: 6rpx solid #ffffff;
}

.item {
  background-repeat: no-repeat;
  background-position: center;
  background-size: 100% 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding-left: 19rpx;
  padding-top: 30rpx;
  padding-bottom: 10rpx;
  box-sizing: border-box;
  /* border: 1rpx solid red; */
  /* min-height: 230rpx; */
}

.item .title {
  font-size: 28rpx;
  font-weight: 700;
  text-align: left;
  color: #333333;
}

.item .name {
  font-size: 20rpx;
  font-weight: 400;
  text-align: left;
  color: #333333;
}

.item .explain {
  font-size: 24rpx;
  font-weight: 500;
  text-align: left;
  color: #6E6E6E;
  padding-left: 14rpx;
}


.list .item .item_left {
  min-height: 210rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  /* padding-top: 27rpx; */
  padding-bottom: 27rpx;
  box-sizing: border-box;
}

.orderNumber {
  font-size: 24rpx;
  font-weight: 500;
  /* color: #008B0A; */
  color: #6E6E6E;
  padding-left: 14rpx;
}

.usedTime {
  font-size: 18rpx;
  font-weight: 400;
  text-align: left;
  color: #333333;
  background-repeat: no-repeat;
  background-size: contain;
  background-position: top;
  margin-top: 4rpx;
  padding-left: 14rpx;
  padding-top: 4rpx;
  padding-bottom: 10rpx;
}

.noData {
  font-size: 24rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-weight: 400;
  color: #707070;
}

.stampBox {
  width: 210rpx;
  /* height: 185rpx; */
