}

.font36 {
  font-size: 36rpx;
}

.font28 {
  font-size: 28rpx;
}

.font20 {
  font-size: 20rpx;
}

.fontW700 {
  font-weight: 700;
}

.fontBold {
  font-weight: bold;
}

.fontNormal {
  font-weight: normal;
}

/* 惠让券列表 */
