}


.list-item-btn {
  display: flex;
  justify-content: space-between;
  margin-top: 14rpx;
  position: relative;
  z-index: 99;
  padding-left: 220rpx;
}
.list-item-btn-dy {
  display: flex;
  justify-content:flex-end;
  margin-top: 14rpx;
  z-index: 99;
}
.list-item-btn .item-btn-left {
  flex: 0.35;
  padding: 0 20rpx;

}

.list-item-btn .item-btn-right {
  flex: 1;
}

.list-item-btn .btn-cancel {
  display: inline-block;
