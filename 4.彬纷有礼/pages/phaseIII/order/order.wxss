/* pages/phaseIII/order/order.wxss */
.navBarTitle {
  color: #ffffff;
  padding: 0px;
}

.left-icon {
  display: flex;
  justify-content: center;
}

.head-fixed {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  height: 160rpx;
  background-color: #fff;
  z-index: 100;
}

.date-box {
  display: flex;
  justify-content: space-between;
  padding: 16rpx 20rpx 0 20rpx;
  font-size: 24rpx;
  color: #333;
}

.date-box .date-left {
  background-color: #F5F5F5;
  border-radius: 24rpx;
  /* width: 316rpx; */
  height: 46rpx;
  padding: 0 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.date-box .date-left .date-icon {
  width: 28rpx;
  height: 28rpx;
  margin-right: 20rpx;
}

.tab-box {
  margin-top: 40rpx;
  margin-left: 34rpx;
  margin-right: 34rpx;
  color: #333;
  font-size: 28rpx;
  display: flex;
  justify-content: space-between;
}

.tab-box .tab-item {
  margin-bottom: 8rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: relative;
}

.tab-box .tab-item .reddot {
  position: absolute;
  right: -34rpx;
  top: -10rpx;
  background-color: #e10404;
  color: #fff;
  border-radius: 50rpx;
  min-width: 35rpx;
  min-height: 35rpx;
  font-size: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.tab-box .tab-active {
  color: #E10404;
  margin-bottom: 0rpx;
}

.tab-box .tab-bottom {
  height: 1px;
  width: 42rpx;
  border-bottom: 6rpx solid #E10404;

}

.list-box {
  position: relative;
  margin-top: 190rpx;
  box-sizing: border-box;
}

.list-bottom {
  padding-bottom: 120rpx;
  /* margin-bottom: 120rpx; */
}

/* .list-box .list-item {
  padding: 0 30rpx;
} */

.list-box .list-item .list-item-content {
  display: flex;
  justify-content: space-between;
  color: #333;
  font-size: 24rpx;
}

.list-box .list-item-content-left {
  display: flex;
  justify-content: center;
  align-items: center;
  flex: 0.35;
  padding: 0 20rpx;
  flex-direction: column;
}

.list-box .list-item-content-left-finish {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  flex: 0.45;
  /* padding: 0 20rpx; */
}

.list-box .item-num {
  font-size: 40rpx;
  font-weight: bold;
}

.list-box .item-type {
  font-size: 24rpx;
  font-weight: normal;
  margin-left: 10rpx;
}

.list-box .list-item-content-right {
  font-size: 24rpx;
  flex: 1;
  position: relative;
}
.list-box .list-item-dy{
  position: relative;
  flex: 1;
  margin-left: 34rpx;
  margin-right: 34rpx;
  /* margin-bottom: 20rpx; */
