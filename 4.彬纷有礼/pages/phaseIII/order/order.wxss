/* orderPop css */
.orderPop {
  height: 850rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  min-width: 92%;
}

.success-icon {
  width: 142rpx;
  height: 142rpx;
}

.cancelOrder-icon {
  width: 192rpx;
  height: 152rpx;
}

.remind-icon {
  width: 240rpx;
  height: 220rpx;
}

.hx-icon {
  width: 148rpx;
  height: 146rpx;
}

.orderPop .orderPop-title {
  font-size: 48rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  color: #333;
}

.orderPop .orderPop-tips {
  font-size: 36rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 500;
  color: #333;
  margin-top: 10rpx;
}

.orderPop .orderPop-box {
  font-size: 32rpx;
  font-weight: 500;
  color: #333;
  text-align: left;
  margin-top: 20rpx;
}

.orderPop .orderNum {
  color: #ec0000;
}

.orderPop .font48 {
  font-size: 48rpx;
}

.orderPop .orderPop-btn {
  display: flex;
  justify-content: space-between;
  width: 100%;
  margin-top: 56rpx;
}

.orderPop .orderPop-confirm {
  display: inline-block;
  border-radius: 50rpx;
  width: 240rpx;
  height: 84rpx;
  line-height: 84rpx;
  background: linear-gradient(180deg, #ff0000, #ac0000);
  color: #ffffff;
  font-size: 32rpx;
}

.orderPop .orderPop-btn .orderPop-cancel {
  display: inline-block;
  border-radius: 50rpx;
  width: 240rpx;
  height: 84rpx;
  line-height: 84rpx;
  background: linear-gradient(180deg, #dcdcdc, #898989);
  color: #ffffff;
  font-size: 32rpx;

}

.orderPop .iknow {
  width: 264rpx;
  height: 84rpx;
  color: #ffffff;
  line-height: 84rpx;
  font-size: 32rpx;
  font-weight: normal;
  background: linear-gradient(180deg, #ff0000 2%, #a90000);
  border-radius: 42rpx;
  display: inline-block !important;
  flex: unset;
}

.successPop {
  height: 600rpx;
}

.mgt30 {
  margin-top: 30rpx;
}

.mgt92 {
  margin-top: 92rpx;
}

.mgt42 {
  margin-top: 42rpx;
}

.orderPop .orderPop-btn .orderPop-btn-mini {
  width: 200rpx;
}

.orderPop .pdlr80 {
  padding-left: 80rpx;
  padding-right: 80rpx;
  box-sizing: border-box;
}

.font72 {
  font-size: 72rpx;
}

.text-red {
  color: #e02020;
}

.text-green {
  color: #079f01;
}

.flex-center {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.footer {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 50rpx;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
  font-size: 24rpx;
  color: #333;
  box-shadow: 0px -6rpx 10rpx 5rpx #f6f6f6;
}

.footer .item {
  flex: 1;
}

.footer .item .num {
  font-size: 28rpx;
  font-weight: bold;
}
