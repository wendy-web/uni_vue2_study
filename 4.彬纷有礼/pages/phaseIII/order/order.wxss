}
.list-item-btn .btn-operate-active {
  display: inline-block;
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 24rpx;
  font-weight: 500;
  background: #e10404;
  color: #fff;
  border-radius: 16px;
  z-index: 99;
}

.list-item-btn .btn-operate-disabled {
  background-color: #c8c8c8;
  color: #fff;
}


.list-box .list-item .dsl-stamp {
