/* 重写UI样式 */
.container {
  position: relative;
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
}

.navbar-box {
  height: 84px;
  display: flex;
  align-items: flex-end;
  justify-content: center;
  color: #ffffff;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;

}

.bg-halo {
  width: 750rpx;
  height: 800rpx;
  z-index: 0;
}

.rank-box {
  position: absolute;
  bottom: 0;
  left: 20rpx;
  right: 20rpx;
  box-sizing: border-box;
  z-index: 1;
}


.stage-head-box {
  position: relative;
  box-sizing: border-box;
  z-index: 2;
}

.bg-stage-head {
  width: 100%;
  height: 126rpx;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 0;
}
.stage-body-box{
  position: relative;
  padding-bottom: 26rpx;
}
.bg-stage-body {
  width: 100%;
  height: 100%;
  position: absolute;
  bottom: 0rpx;
  left: 0rpx;
  right: 0rpx;
  top: 0;
  z-index: -1;
}

