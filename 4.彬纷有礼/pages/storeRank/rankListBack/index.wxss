  justify-content: center;
  font-size: 28rpx;
  font-weight: 500;
  color: #bb5c38;
}

.myrank-item {
  background-color: #d53b37;
  border-radius: 10rpx;
  color: #ffffff;
  box-shadow: 5rpx 5rpx 16rpx 0px rgba(0, 0, 0, 0.33);
}

.myrank-row {
  background: #FAC5B4;
  border-radius: 0 0 35rpx 35rpx;
  height: 100rpx;
  padding: 0 18rpx 0 10rpx;
  font-size: 28rpx;
  font-weight: 700;
  color: #BB5C38;
  box-sizing: border-box;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-shrink: 0;
  text-align: center;
  z-index: 2;
}
.myrank-num {
  text-align: center;
