  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #ffffff;
  line-height: 32rpx;
  position: relative;
  z-index: 10;
}

/* table 列表 */
.table-box {
  display: flex;
  flex-direction: column;
  background: #fae3c6;
  border-radius: 44rpx;
  box-shadow: 0rpx 6rpx 12rpx 0rpx rgba(0, 0, 0, 0.33);
  box-sizing: border-box;
  margin: 0 15rpx;
  overflow: hidden;
}

.table-head {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 26rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Heavy;
  font-weight: 800;
  color: #961b05;
  text-align: center;
  box-sizing: border-box;
  padding: 30rpx 0;
}

.table-title-rank {
  min-width: 120rpx;
  flex-shrink: 0;
}

.table-title-shop {
  max-width: 280rpx;
  flex-shrink: 0;
}

.table-title-unbox {
  min-width: 200rpx;
  flex-shrink: 0;
}

.list-box {
  height: var(--max-height);
}

.list-item-box {
  font-size: 26rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  color: #c16b48;
  background-image: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAATcAAAABCAYAAAC7duk2AAAAMUlEQVQ4T2PcMbXsHwMDIyMDEcAju5ModUQYxTBqL+5QGg1nYlIQdjWj6Wo0XcFCAAAn4irFFughhQAAAABJRU5ErkJggg==");
  background-position: bottom;
  background-repeat: no-repeat;
  background-size:90%;
  margin-left: 10rpx;
  margin-right: 10rpx;
  box-sizing: border-box;
}

.list-item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  text-align: center;
  min-height: 80rpx;
}

.list-item-center {
  display: flex;
  align-items: center;
  text-align: center;
  min-height: 80rpx;
  justify-content: center;
  font-size: 26rpx;
  font-weight: 500;
  color: #c16b48;
}
.divider-box{
  box-sizing: border-box;
  padding: 0 100rpx;
}
.loading-box{
  text-align: center;
}
.rank-num {
  font-size: 26rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Heavy;
  font-weight: 800;
