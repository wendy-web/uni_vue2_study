/* 2023年8月12日 */
.typePop-box {
  margin: 0rpx 50rpx;
  padding-top: 20rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;

  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Medium;
  font-weight: 500;
  text-align: center;
  color: #2a2a2a;
  max-height: 400rpx;
  overflow: scroll;
}

.typePop-title {
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  text-align: left;
  color: #000000;
  text-align: center;
  position: relative;
  min-height: 50rpx;
  line-height: 50rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
  margin-top: 20rpx;
}

.typePop-title::after {
  content: '';
  position: absolute;
  left: 50rpx;
  right: 50rpx;
  bottom: -10rpx;
  width: 650rpx;
  height: 0rpx;
  border-bottom: 1rpx solid #d8d8d8;
  margin: 0 auto;
}

.typePop-box .typePop-box-item {
  background-color: #fff;
  color: #2A2A2A;
  width: 204rpx;
  height: 80rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-bottom: 20rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 0px 10rpx;
}

.typePop-box .activeType {
  background-color: #e73636;
  color: #fff;
}

.typePop-box .typePop-box-item-crew {
  background-color: #fff;
  color: #333;
  width: 204rpx;
  height: 78rpx;
  font-size: 28rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-bottom: 20rpx;
}

.typePop-box-item .crew-item {
  width: 100%;
  text-align: center;
}

/* css flex space-between 居左实现 
3n+1时候显示占位(flex-start效果)
*/
.typePop-box .typePop-box-item-placeholder {
  visibility: hidden;
}

/* 3n+2的时候:即满足nth-child又满足nth-last-child的前提下隐藏最后1个占位 */
.typePop-box-item:nth-child(3n+2):nth-last-child(3)+.typePop-box-item-placeholder+.typePop-box-item-placeholder {
  display: none;
}

/* 3n+3的时候隐藏2个占位 */
.typePop-box-item:nth-child(3n):nth-last-child(3)+.typePop-box-item-placeholder,
.typePop-box-item:nth-child(3n):nth-last-child(3)+.typePop-box-item-placeholder+.typePop-box-item-placeholder {
  display: none;
}

