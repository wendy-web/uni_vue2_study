.marquee_wp .marquee_item image {
  display: block;
  width: 100%;
  height: 100%;
}
.marquee_wp .marquee_item.on image{
  position: relative;
  z-index: 2;
  width: 235rpx;
  height: 270rpx;
  left: -35rpx;
  top: -48rpx;
}
.marquee_wp .marquee_item:nth-child(3n) {
  margin-right: 0;
}

/* .marquee_wp .marquee_item_btn {
  background: transparent;
} */

.marquee_wp .marquee_item_btn text {
  display: block;
  text-align: center;
  color: #fff;
}
.on{
  z-index: 10;
}
.game-mask{
  position: fixed;
  z-index: 3;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0,0,0,.4);
}