.marquee_wp {
  margin: 0 20rpx;
  display: flex;
  flex-wrap: wrap;
  /* height: 646rpx; */
  height: 600rpx;
  justify-content: space-around;
  align-items: center;
}

.marquee_wp .marquee_item {
  width: 162rpx;
  height: 170rpx;
  margin: 0 10rpx 10rpx 0;
  box-sizing: border-box;
  background: url('data:image/png;base64,') no-repeat;
  background-size: contain;
  background-position: center;
}

