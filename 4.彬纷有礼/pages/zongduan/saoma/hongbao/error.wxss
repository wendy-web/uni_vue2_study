/* 自定义蒙层 */
.bgMask {
  position: absolute;
  top: 0%;
  left: 0%;
  width: 100%;
  height: 100%;
  background-color: black;
  z-index: 10;
  -moz-opacity: 0.7;
  opacity: 0.7;
  filter: alpha(opacity=70);
}

.frontMask {
  text-align: center;
  position: absolute;
  /* top: 64%; */
  bottom: 0%;
  /* left: 22%; */
  /* width: 47%; */
  width: 100%;
  height: 10%;
  padding: 8px;
  border-radius: 8px;
  /* border: 2px solid rgba(255, 255, 255, 1); */
  /* opacity: 0.79; */
  /* background-color:  transparent; */
  background-color: black;
  opacity: 0.7;
  filter: alpha(opacity=70);
  -moz-opacity: 0.7;
  z-index: 11;
  overflow: auto;
}

