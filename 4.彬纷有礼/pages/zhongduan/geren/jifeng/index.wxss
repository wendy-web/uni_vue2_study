/* newUI  start */

.credits-head {
  color: #fff;
  /* background: #e73636; */
  /*#0396FF*/
  background-color: #fff;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 100% 100%;
  width: 100%;
  height: 347rpx;
  position: relative;
  padding-top: 50rpx;
  box-sizing: border-box;
  position: fixed;
  top: 0;
}

.credits-head .credits-store {
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #ffd9d5;
  width: 198rpx;
  height: 68rpx;
  border-radius: 0 50rpx 50rpx 0;
  color: #E10404;
  font-size: 28rpx;
  font-weight: 700;
}

.credits-store .credits-store-icon {
  width: 37rpx;
  height: 32rpx;
  margin-right: 12rpx;
}

.credits-head .credits-info {
  /* margin-top: 10rpx; */
  margin-top: -20rpx;
  font-size: 32rpx;
  text-align: center;
}

.credits-head .credits-info .credits-count {
  font-size: 68rpx;
}

.credits-info .credits-tips {
  display: flex;
  justify-content: center;
  align-items: center;
}

.credits-info .credits-tips-title {
  margin-left: 10rpx;
  margin-right: 6rpx;
}

.credits-tips .credits-num-icon {
  width: 46rpx;
  height: 48rpx;
}

.credits-tips .credits-tips-icon {
  width: 34rpx;
  height: 34rpx;
}

.credits-water {
  position: absolute;
  left: 0;
  bottom: -10px;
  height: 30px;
  width: 100%;
  z-index: 1;
}

.credits-water-c {
  position: relative;
}

.credits-water-1 {
  background: url("data:image/svg+xml;base64,") repeat-x;
  background-size: 600px;
  -webkit-animation: wave-animation-1 3.5s infinite linear;
  animation: wave-animation-1 3.5s infinite linear;
}

.credits-water-2 {
  top: 5px;
  background: url("data:image/svg+xml;base64,") repeat-x;
  background-size: 600px;
  -webkit-animation: wave-animation-2 6s infinite linear;
  animation: wave-animation-2 6s infinite linear;
}

.credits-water-1,
.credits-water-2 {
  position: absolute;
  width: 100%;
  /* height: 60px; */
  height: 20px;
}

@keyframes wave-animation-1 {
  0% {
    background-position: 0 top;
  }

  100% {
    background-position: 600px top;
  }
}

@keyframes wave-animation-2 {
  0% {
    background-position: 0 top;
  }

  100% {
    background-position: 600px top;
  }
}

.list-box {
  padding-bottom: 10vh;
  margin-top: 367rpx;
}

.list-item {
  margin: 30rpx 44rpx 44rpx 44rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  color: #676767;
}

.list-item .item-title {
  color: #333;
  font-size: 30rpx;
}

.list-item .red {
  color: #E73636;
}

.list-item .green {
  color: #00C70E;
}

.list-box .noMore {
  color: #999;
  font-size: 24rpx;
  text-align: center;
}






/* newUI  end */

.adContainer-bottom {
  width: 100%;
  position: fixed;
  bottom: 0px;
}

.rulePop {
  height: 70vh;
  color: #333;
  font-size: 28rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
  box-sizing: border-box;
  padding: 80rpx 50rpx;
}

.rulePop .rulePop-title {
  font-size: 32rpx;
}

.iknow {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
}

.iknow-btn {
  color: #fff;
  background-color: #e13030;
  width: 240rpx;
  height: 72rpx;
  text-align: center;
  line-height: 72rpx;
  border-radius: 50rpx;
}