page{
  height: 100%;
}


.container {
  display: flex;
  min-height: 100%;
}


.adContainer-bottom {
  width: 100%;
  align-self: flex-end;
}

.content{
  box-sizing: border-box;
  padding: 0px 30rpx;
  min-height: 100vh;
  width: 100%;
  position: absolute;
}

.logo {
  text-align: center;
  padding-top: 100rpx;
  padding-bottom: 100rpx;
}

.logo image {
  width: 160rpx;
  height: 160rpx;

}

.title {
  display: flex;
  -webkit-justify-content: center;
  -ms-flex-pack: center;
  justify-content: center;
  color: #a96f41;
  font-weight: bold;
  font-size: 40rpx;

}

.weui-dialog {
  width: 87%;
  margin: 0 auto;

}
/* 2023年10月19日 个人隐私保护 */
.privacy-box{
  position: relative;
  box-sizing: border-box;
}
.icon-dot{
  width: 20rpx;
  height: 20rpx;
  background-color: #e3001b;
  border-radius: 50rpx;
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  margin: auto;
}